---
title: 'Homepage'
---

{% extends "_layout/base.html.twig" %}
{#{% import "_component/helpers/svg.html.twig" as foobar %}#}
{% include "_component/helpers/svg.html.twig" %}


{% block main %}

<div id="container">
    <!-- This element's contents will be replaced with your component. -->
</div>

{{ svg('icon-fb') }}
{{ svg('icon-tw') }}
{{ svg('icon-yt') }}


<h3>Newss list</h3>
{% for news in newses %}
    <div class="news" style="float:left;width: 100%;">
      <h3>{{ news.title }}</h3>
      <div>{{ news.date }}</div>
      <img src="{{ news.image }}" width="200" alt="{{ news.title }}">
      <p>{{ news.desc|truncate(50) }}</p>
    </div>
{% endfor %}

{% endblock %}
